#include <unistd.h>

void	ft_putnbr(int n)
{
	char c;

	if (n < 0)
	{
		write(1, "-", 1);
		n *= -1;
	}
	if (n > 9)
		ft_putnbr(n / 10);
	c = n % 10 + '0';
	write(1, &c, 1);
}

int	ft_strnequ(char const *s1, char const *s2, size_t n)
{
	size_t i;

	i = 0;
	while (s1[i] == s2[i] && i <= n && n != 0)
	{
		if (!s1[i])
			return (1);
		i++;
	}
	return (0);
}

int		ft_atoi(const char *s)
{
	int i;
	int sign;
	int push;

	i = 0;
	sign = 1;
	push = 0;
	while (s[i] == ' ' ||
			s[i] == '\t' ||
			s[i] == '\r' ||
			s[i] == '\n' ||
			s[i] == '\v' ||
			s[i] == '\f')
		i++;
	if (s[i] == '-')
		sign = -1;
	if (s[i] == '-' || s[i] == '+')
		i++;
	while (s[i] && s[i] >= '0' && s[i] <= '9')
	{
		push = push * 10 + s[i] - '0';
		i++;
	}
	return (push * sign);
}

int	main(int ac, char **av)
{
	ft_putnbr(ftstrnequ(av[1], av[2], av[3][0]));
	write(1, "\n", 1);
	return (0);
}
